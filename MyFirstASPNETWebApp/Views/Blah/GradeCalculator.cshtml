@{
}

@model GradeCalculatorModel;

<h3>Grade Calculator</h3>
<form method="post">
    <label>Assignments: </label>
    <input id="assignments" asp-for="assignments" /><br />
    <label>Group Project: </label>
    <input id="groupproject" asp-for="groupproject"/><br />
    <label>Quizzes: </label>
    <input id="quizzes" asp-for="quizzes"/><br />
    <label>Exams: </label>
    <input id="exams" asp-for="exams"/><br />
    <label>INTEX: </label>
    <input id="intex" asp-for="intex"/><br />
    <input id="btnSend" type="submit" value="calculate" />

    <div asp-validation-summary="All"></div>
</form>
<script src="~/js/GradeCalculator.js"></script>




